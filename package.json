{
  "name": "qr-code-manager",
  "version": "1.0.0",
  "description": "A complete web-based QR code management system for personal use",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "deploy": "bash deploy.sh",
    "build": "npx browserify -r qrcode -o public/js/qrcode-bundle.js --standalone QRCode",
    "postinstall": "npm run build"
  },
  "keywords": [
    "qr-code",
    "qr",
    "generator",
    "manager"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "browserify": "^17.0.1",
    "nodemon": "^3.0.2"
  }
}
